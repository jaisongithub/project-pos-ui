<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - POSAPP</title>
    <environment include="Development">
        <link href="~/css/bootstrap.css" rel="stylesheet" />
        <link href="~/css/mdb.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/css/bootstrap.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/mdb.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/css/style.min.css" asp-append-version="true" />

    </environment>
    <link href="~/css/adminlte.css" rel="stylesheet" />
    <link href="~/css/adminpanelstyles.min.css" rel="stylesheet" />
</head>
<body class="fixed-sn mdb-skin-custom hold-transition sidebar-mini" data-spy="scroll" data-target="#scrollspy" data-offset="15" style="">
    <header class="header posadmin-top-header">
     
    </header>
    <div class="wrapper" style="height:2000px;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2018 - WebApplication3</p>
        </footer>
    </div>
    <environment include="Development">
        <script src="~/js/jquery-3.3.1.min.js"></script>
        <script src="~/js/bootstrap.js"></script>
        <script src="~/js/mdb.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/bootstrap.min.js"></script>
        <script src="~/js/mdb.min.js"></script>
        <script src="~/js/jquery-3.3.1.min.js"></script>
        <script src="~/js/popper.min.js"></script>
        <script src="~/js/site.min.js"></script>
    </environment>
    <script src="~/js/adminpanel/adminlte.min.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
